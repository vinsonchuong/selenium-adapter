{
  "name": "selenium-adapter",
  "version": "0.0.3",
  "description": "A promise adapter for Selenium WebDriver for use with ES2017 async/await",
  "keywords": [
    "selenium",
    "chrome",
    "firefox"
  ],
  "homepage": "https://github.com/vinsonchuong/selenium-adapter",
  "bugs": "https://github.com/vinsonchuong/selenium-adapter/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "src/index.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsonchuong/selenium-adapter"
  },
  "scripts": {
    "test": "standard -v --fix && flow && ava",
    "build": "build-esm",
    "prepublishOnly": "build-esm"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "chromedriver": "^2.29.0",
    "css-to-xpath": "^0.1.0",
    "geckodriver": "^1.6.1",
    "phantomjs-prebuilt": "^2.1.14",
    "selenium-webdriver": "^3.4.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-package-name-import": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "build-esm": "^2.0.2",
    "directory-helpers": "^0.1.8",
    "flow-bin": "^0.46.0",
    "node-static": "^0.7.9",
    "standard-esnext": "^2.0.2",
    "tempfile": "^2.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "flow"
    ],
    "plugins": [
      "transform-runtime",
      "package-name-import"
    ]
  },
  "ava": {
    "concurrency": 8,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
