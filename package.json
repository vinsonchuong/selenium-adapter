{
  "name": "selenium-adapter",
  "version": "0.2.7",
  "description": "A promise adapter for Selenium WebDriver for use with ES2017 async/await",
  "keywords": [
    "selenium",
    "chrome",
    "firefox"
  ],
  "homepage": "https://github.com/vinsonchuong/selenium-adapter",
  "bugs": "https://github.com/vinsonchuong/selenium-adapter/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "src/index.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsonchuong/selenium-adapter"
  },
  "scripts": {
    "test": "bin/makeindex src/actions && standard -v --fix && flow && ava",
    "prepack": "build-esm"
  },
  "dependencies": {
    "chromedriver": "^2.32.3",
    "css-to-xpath": "^0.1.0",
    "geckodriver": "^1.9.0",
    "phantomjs-prebuilt": "^2.1.15",
    "selenium-webdriver": "^3.5.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-preset-diff": "^1.0.3",
    "babel-register": "^6.26.0",
    "build-esm": "^3.1.0",
    "directory-helpers": "^0.1.8",
    "flow-bin": "^0.56.0",
    "node-static": "^0.7.10",
    "standard-esnext": "^4.0.1",
    "tempfile": "^2.0.0"
  },
  "babel": {
    "presets": [
      "diff"
    ]
  },
  "ava": {
    "concurrency": 8,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
